    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug blinky blank",
            "cwd": "${workspaceRoot}/examples/peripheral/blinky",
            "executable": "${workspaceRoot}/examples/peripheral/blinky/pca10040/blank/armgcc/_build/nrf52832_xxaa.out",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "device": "nrf52",
            "interface": "swd",
            "runToEntryPoint": "main",
            "armToolchainPath": "/usr/bin",
            "svdFile": "${workspaceRoot}/SVD/nrf52.svd"
        },
        {
            "name": "Cortex Debug blinky s132",
            "cwd": "${workspaceRoot}/examples/peripheral/blinky",
            "executable": "${workspaceRoot}/examples/peripheral/blinky/pca10040/s132/armgcc/_build/nrf52832_xxaa_s132.out",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "device": "nrf52",
            "interface": "swd",
            "runToEntryPoint": "main",
            "armToolchainPath": "/usr/bin",
            "svdFile": "${workspaceRoot}/SVD/nrf52.svd"
        },
        {
            "name": "Cortex Debug experimental ble app blinky",
            "cwd": "${workspaceRoot}/examples/ble_peripheral/experimental_ble_app_blinky",
            "executable": "${workspaceRoot}/examples/ble_peripheral/experimental_ble_app_blinky/pca10040/s132/armgcc/_build/nrf52832_xxaa_s132.out",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "device": "nrf52",
            "interface": "swd",
            "runToEntryPoint": "main",
            "armToolchainPath": "/usr/bin",
            "svdFile": "${workspaceRoot}/SVD/nrf52.svd"
        }
    ]
}
